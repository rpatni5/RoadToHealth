<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <LinearGradientBrush x:Key="DashboardStartColor" StartPoint="1,0" EndPoint="1,1">
        <GradientStop Color="#f48946" Offset="1.0" />
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="DashboardBGColor" StartPoint="1,0" EndPoint="1,1">
        <GradientStop Color="#f48946" Offset="0.5" />
        <GradientStop Color="#f85846" Offset="1.0" />
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="InsightBGColor" StartPoint="1,0" EndPoint="1,1">
        <GradientStop Color="#86cbca" Offset="0.5" />
        <GradientStop Color="#c3e5e3" Offset="1.0" />
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="ResultBGColor" StartPoint="1,0" EndPoint="1,1">
        <GradientStop Color="#7dcacc" Offset="0.5" />
        <GradientStop Color="#50acb2" Offset="1.0" />
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="PHABGColor" StartPoint="1,0" EndPoint="1,1">
        <GradientStop Color="#f48946" Offset="0.5" />
        <GradientStop Color="#f85846" Offset="1.0" />
    </LinearGradientBrush>
</ResourceDictionary>